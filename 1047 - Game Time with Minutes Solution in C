#include<stdio.h>
int main(){
int start_hour,end_hour,start_munit,end_munit,duration;
scanf("%d %d %d %d",&start_hour,&start_munit,&end_hour,&end_munit);
duration=(end_hour*60+end_munit)-(start_hour*60+start_munit);
if(duration<=0){
    duration+=24*60;
}printf("O JOGO DUROU %d HORA(S) E %d MINUTO(S)\n",duration/60,duration%60);

return 0;
}
