#include<stdio.h>
int main(){
int row,col,px=5,py=6;
double N[12][12],sum,avg;
char ch[2];
scanf("%s",&ch);
for(row=0;row<12;row++){
    for(col=0;col<12;col++){
        scanf("%lf",&N[row][col]);
    }
}

sum=0.0;
for(row=7;row<=11;row++){
    for(col=px;col<=py;col++){
        sum=sum+N[row][col];
    }
    px--;
    py++;
}

if(ch[0]=='S'){
    printf("%.1lf\n",sum);
}else if(ch[0]=='M'){
    avg=sum/30.0;
        printf("%.1lf\n",avg);
}
return 0;
}

