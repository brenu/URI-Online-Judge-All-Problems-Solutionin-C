#include<stdio.h>
int main(){
int n,row,col;
double array[12][12],sum,avg;
 char ch[2];
scanf("%d %s",&n,&ch);
for(row=0;row<12;row++){
    for(col=0;col<12;col++){
        scanf("%lf",&array[row][col]);
    }
}


sum=0.0;

    for(col=0;col<12;col++){
      sum= sum+array[n][col];

}

if(ch[0]=='S'){
    printf("%.1lf\n",sum);
}else if(ch[0]=='M'){
      avg=sum/12.0;
        printf("%.1lf\n",avg);
}

return 0;
}
